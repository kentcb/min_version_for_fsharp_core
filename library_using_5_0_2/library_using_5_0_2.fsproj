<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <!--
  We want this project to use FSharp.Core 5.0.2 rather than the default of 4.7.2 established by the Directory.Build.* files.
  -->
  <ItemGroup>
    <PackageReference Update="FSharp.Core" VersionOverride="5.0.2" />
    <PackageReference Include="FSharpPlus" Version="1.2.1" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="Library.fs" />
  </ItemGroup>

</Project>
